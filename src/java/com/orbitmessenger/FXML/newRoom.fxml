<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.ToggleSwitch?>

<VBox fx:id="mainVBox" alignment="CENTER" prefHeight="200.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="14.0" layoutY="41.0" text="Room Name" />
                <TextField fx:id="roomNameTxtField" layoutX="161.0" layoutY="36.0" prefHeight="26.0" prefWidth="55.0" />
                <Button fx:id="createRoomBtn" layoutX="170.0" layoutY="155.0" mnemonicParsing="false" onAction="#createRoom" onKeyPressed="#createRoom" text="Create Room" />
            </children>
        </AnchorPane>
    </children>
</VBox>
