<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainVBox" alignment="CENTER" prefHeight="100.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="31.0" layoutY="14.0" text="Room Name" />
                <TextField fx:id="roomNameTxtField" layoutX="148.0" layoutY="9.0" prefHeight="26.0" onKeyPressed="#handleEnterPressed" prefWidth="200.0" />
                <Button fx:id="createRoomBtn" layoutX="148.0" layoutY="50.0" mnemonicParsing="false" onMouseClicked="#createRoom" text="Create Room" />
            </children>
        </AnchorPane>
    </children>
</VBox>
